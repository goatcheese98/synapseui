#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rohanjasani/Projects/myfirstcli/synapseui/node_modules/.pnpm/histoire@1.0.0-alpha.2_@types+node@24.1.0_vite@6.3.5_@types+node@24.1.0_jiti@2.5.1_yaml@2.8.0_/node_modules/histoire/node_modules:/Users/rohanjasani/Projects/myfirstcli/synapseui/node_modules/.pnpm/histoire@1.0.0-alpha.2_@types+node@24.1.0_vite@6.3.5_@types+node@24.1.0_jiti@2.5.1_yaml@2.8.0_/node_modules:/Users/rohanjasani/Projects/myfirstcli/synapseui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rohanjasani/Projects/myfirstcli/synapseui/node_modules/.pnpm/histoire@1.0.0-alpha.2_@types+node@24.1.0_vite@6.3.5_@types+node@24.1.0_jiti@2.5.1_yaml@2.8.0_/node_modules/histoire/node_modules:/Users/rohanjasani/Projects/myfirstcli/synapseui/node_modules/.pnpm/histoire@1.0.0-alpha.2_@types+node@24.1.0_vite@6.3.5_@types+node@24.1.0_jiti@2.5.1_yaml@2.8.0_/node_modules:/Users/rohanjasani/Projects/myfirstcli/synapseui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../histoire/bin.mjs" "$@"
else
  exec node  "$basedir/../histoire/bin.mjs" "$@"
fi
